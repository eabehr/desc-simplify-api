{
    "name": "desc-simplify-api",
    "version": "1.0.0",
    "description": "RESTful json API for the DESC simplify project",
    "main": "index.js",
    "scripts": {
        "prebuild": "npm run clean",
        "build": "babel src -d build -s",
        "clean": "rm -rf build && mkdir build",
        "serve:dev": "DEBUG=app babel-node src/server.js",
        "prestart": "npm run build",
        "start": "node build/server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jasonsjones/desc-simplify-api.git"
    },
    "author": "Jason S. Jones",
    "license": "MIT",
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-env": "^2.4.1",
        "dotenv": "^6.0.0",
        "ejs": "^2.6.1",
        "express": "^4.16.3",
        "morgan": "^1.9.0"
    },
    "devDependencies": {
        "debug": "^3.1.0"
    }
}
