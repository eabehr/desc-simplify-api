{
    "name": "desc-simplify-api",
    "version": "1.0.0",
    "description": "RESTful json API for the DESC simplify project",
    "main": "index.js",
    "scripts": {
        "prebuild": "npm run clean",
        "build": "babel src -d build --ignore *test.js",
        "clean": "rimraf build",
        "serve:dev": "DEBUG=app babel-node src/server.js",
        "preserve:prod": "npm run build",
        "serve:prod": "node build/server.js",
        "start": "npm run serve:dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jasonsjones/desc-simplify-api.git"
    },
    "author": "Jason S. Jones",
    "license": "MIT",
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-env": "^2.4.1",
        "dotenv": "^6.0.0",
        "ejs": "^2.6.1",
        "express": "^4.16.3",
        "morgan": "^1.9.0",
        "rimraf": "^2.6.2"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.6",
        "debug": "^3.1.0",
        "eslint": "^5.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-prettier": "^2.6.2",
        "prettier": "^1.13.7"
    }
}
